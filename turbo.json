{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "stream",
  "tasks": {
    "@cowk8s/web#go": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@cowk8s/database#db:setup", "@cowk8s/js#build"]
    },
    "@cowk8s/database#build": {
      "cache": false,
      "dependsOn": ["post-install"]
    },
    "@cowk8s/js#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "@cowk8s/surveys#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "@cowk8s/types#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "DATABASE_URL"
      ]
    },
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["dist/**", ".next/**"]
    },
    "post-install": {
      "cache": false,
      "dependsOn": [],
      "outputs": [],
      "inputs": []
    },
    "db:migrate:dev": {
      "outputs": []
    },
    "db:start": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}